    "@tarojs/plugin-platform-h5": "3.6.4",
    "@tarojs/plugin-platform-jd": "3.6.4",
    "@tarojs/plugin-platform-qq": "3.6.4",
    "@tarojs/plugin-platform-swan": "3.6.4",
    "@tarojs/plugin-platform-tt": "3.6.4",
    "@tarojs/plugin-platform-weapp": "3.6.4",
    "@tarojs/runtime": "3.6.4",
    "@tarojs/shared": "3.6.4",
    "@tarojs/taro": "3.6.4",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "pinia": "^2.0.35",
    "qs": "^6.11.2",
    "vue": "^3.2.40"
